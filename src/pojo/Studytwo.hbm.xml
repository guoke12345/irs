<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Studytwo" table="studytwo">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity"></generator>
        </id>
         <property name="studyTwoName" type="java.lang.String">
            <column name="studytwoname" length="255" />
        </property>
        <property name="studyOneName_id" type="java.lang.Integer">
            <column name="studyonename_id" length="11" />
        </property>
    </class>
</hibernate-mapping>