<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Carousel" table="carousel">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity"></generator>
        </id>
         <property name="carouselName" type="java.lang.String">
            <column name="carouselname" length="255" />
        </property>
        <property name="carouselPhoto" type="java.lang.String">
            <column name="carouselphoto" length="255" />
        </property>
        <property name="carouselTime" type="java.lang.String">
            <column name="carouseltime" length="255" />
        </property>
        <property name="carouselContent" type="java.lang.String">
            <column name="carouselcontent" length="100000" />
         </property>
        <property name="carouselNum" type="java.lang.Integer">
            <column name="carouselnum" length="11" />
        </property>
    </class>
</hibernate-mapping>